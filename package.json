{
  "name": "jstest",
  "version": "0.0.1",
  "description": "pivoted ssr for jifflenow assignment",
  "private": true,
  "engines": {
    "node": ">=8.4",
    "npm": ">=5.3"
  },
  "main": "main.js",
  "scripts": {
    "test": "mocha --recursive --compilers js:babel-core/register tests",
    "start": "nodemon --exec babel-node src/server.js",
    "start-trace": "nodemon --exec babel-node --stack-trace-limit=1000 src/server.js",
    "build-server": "babel src --out-dir build",
    "build-client": "webpack --progress --display-error-details",
    "build-client-watch": "webpack --progress --watch",
    "devStart": "better-npm-run devBuild",
    "bp": "better-npm-run localDev",
    "lint-fix": "eslint --fix",
    "precommit": "npm run lint",
    "lint": "eslint src"
  },
  "betterScripts": {
    "devBuild": {
      "command": "npm run build-client -- --watch & nodemon --exec babel-node src/server.js",
      "env": {}
    },
    "localDev": {
      "command": "npm run build-client-watch & npm run start-trace",
      "env": {}
    }
  },
  "keywords": [
    "nodejs",
    "react",
    "redux"
  ],
  "author": "bp <bharat@zivame.com>",
  "license": "ISC",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.2.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "mocha": "^3.5.0",
    "react-test-renderer": "^15.6.1",
    "selenium-standalone": "^6.7.0",
    "style-loader": "^0.18.2",
    "wdio-dot-reporter": "0.0.9",
    "wdio-mocha-framework": "^0.5.11",
    "wdio-selenium-standalone-service": "0.0.9",
    "webdriverio": "^4.8.0",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0"
  },
  "dependencies": {
    "autoprefixer": "^7.1.4",
    "babel-eslint": "^7.2.3",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "better-npm-run": "^0.1.0",
    "body-parser": "^1.17.2",
    "connect-redis": "^3.3.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "fs-extra": "^4.0.1",
    "helmet": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "^1.8.2",
